generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id      String @id @default(uuid())
  apelido String
  email   String
  senha   String

  created_at DateTime @default(now())
  updated_at DateTime @default(now())

  @@map("users")
}
